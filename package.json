{
  "name": "react-pro",
  "version": "1.0.0",
  "description": "An out-of-box UI solution for enterprise applications",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run dev\" \"nodemon mock/index.js\"",
    "dev": "cross-env webpack server --config webpack.dev.config.js",
    "build": "cross-env webpack --config webpack.prod.config.js",
    "analyze": "ANALYZE=true npm run build",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:js": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty",
    "lint:style": "stylelint --fix \"src/**/*.{css,less}\"",
    "lint:prettier": "prettier --check --write '**/*.{js,jsx,ts,tsx,css,less}'",
    "prepare": "husky install"
  },
  "keywords": [
    "react",
    "webpack5"
  ],
  "author": "shizhiqian",
  "license": "ISC",
  "private": true,
  "browserslist": [
    "last 2 versions",
    "> 2%",
    "not ie <= 10"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix",
    "src/**/*.{less,css}": "stylelint --fix",
    "src/**/*.{js,jsx,ts,tsx,less,css,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "@rematch/core": "^2.0.1",
    "@rematch/loading": "^2.0.1",
    "antd": "^4.16.3",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "core-js": "^3.15.2",
    "dayjs": "^1.10.5",
    "history": "^5.0.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-cache-route": "^1.11.1",
    "react-router-dom": "^5.2.0",
    "react-use": "^17.2.4",
    "redux": "^4.1.0",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "@types/loadable__component": "^5.13.4",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "mockjs": "^1.1.0",
    "nodemon": "^2.0.9",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.1",
