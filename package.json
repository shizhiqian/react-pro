{
  "name": "react-pro",
  "version": "1.0.0",
  "description": "An out-of-box UI solution for enterprise applications",
  "main": "index.js",
  "scripts": {
    "start": "webpack server --config webpack.dev.config.js",
    "build": "webpack --config webpack.production.config.js",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:js": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty",
    "lint:style": "stylelint --fix \"src/**/*.{css,less}\"",
    "lint:prettier": "prettier --check --write '**/*.{js,jsx,ts,tsx,css,less}'",
    "prepare": "husky install",
    "analyze": "cross-env NODE_ENV=production npm_config_report=true npm run build"
  },
  "keywords": [
    "react",
    "webpack5"
  ],
  "author": "shizhiqian",
  "license": "ISC",
  "private": true,
  "browserslist": [
    "last 2 versions",
    "> 2%",
    "not ie <= 10"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix",
    "src/**/*.{less,css}": "stylelint --fix",
    "src/**/*.{js,jsx,ts,tsx,less,css,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@rematch/core": "^2.0.1",
    "antd": "^4.16.3",
    "axios": "^0.21.1",
    "dayjs": "^1.10.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "core-js": "^3.14.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "mockjs": "^1.1.0",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "terser-webpack-plugin": "^5.1.3",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "4.0.0-beta.3",
    "webpackbar": "^5.0.0-3"
  }
}
