    "stylelint-order": "^4.1.0",
    "terser-webpack-plugin": "^5.1.3",
    "ts-import-plugin": "^1.6.7",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-dev-server": "4.0.0-beta.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpackbar": "^5.0.0-3"
  }
}
